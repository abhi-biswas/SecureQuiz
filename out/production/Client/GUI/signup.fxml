<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.signup">
   <children>
      <Button fx:id="BackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
      <TextField id="Email" fx:id="Email" layoutX="180.0" layoutY="243.0" prefHeight="25.0" prefWidth="441.0" promptText="Enter E-mail Id">
         <font>
            <Font size="27.0" />
         </font>
      </TextField>
      <TextField id="Email" fx:id="fname" layoutX="180.0" layoutY="57.0" prefHeight="25.0" prefWidth="441.0" promptText="Enter First Name">
         <font>
            <Font size="27.0" />
         </font>
      </TextField>
      <TextField id="Email" fx:id="lname" layoutX="180.0" layoutY="147.0" prefHeight="25.0" prefWidth="441.0" promptText="Enter Last Name">
         <font>
            <Font size="27.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="180.0" layoutY="348.0" prefHeight="25.0" prefWidth="441.0" promptText="Enter Password">
         <font>
            <Font size="27.0" />
         </font>
      </PasswordField>
      <Button fx:id="SignUpButton" layoutX="313.0" layoutY="499.0" mnemonicParsing="false" onAction="#signUpButtonClicked" prefHeight="25.0" prefWidth="175.0" text="SignUp">
