<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.addQuiz">
   <content>
      <AnchorPane prefHeight="658.0" prefWidth="836.0">
         <children>
            <TextField fx:id="quizNameTextField" layoutX="221.0" layoutY="83.0" prefHeight="25.0" prefWidth="404.0" promptText="Enter Quiz Name">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
            <Label layoutX="54.0" layoutY="76.0" prefHeight="44.0" prefWidth="119.0" text="Quiz Name">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="addButton" layoutX="30.0" layoutY="14.0" mnemonicParsing="false" onAction="#addButtonClicked" prefHeight="44.0" prefWidth="167.0" text="Add" />
            <Button fx:id="backButton" layoutX="637.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="44.0" prefWidth="167.0" text="Back" />
            <Label layoutX="54.0" layoutY="131.0" prefHeight="44.0" prefWidth="119.0" text="Subject Code:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label fx:id="subjectCodeLabel" layoutX="221.0" layoutY="131.0" prefHeight="44.0" prefWidth="119.0" text="Subject Code:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <VBox fx:id="questionBox" layoutX="30.0" layoutY="200.0" />
            <TextField fx:id="maxMarksTextField" layoutX="418.0" layoutY="136.0" prefHeight="35.0" prefWidth="359.0" promptText="Enter Maximum Marks">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
